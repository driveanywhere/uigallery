{
  "presets": ["@babel/react", "@babel/typescript", [
    "@babel/preset-env",
    {
      "useBuiltIns": "entry"
    }
  ]],
  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread",
    "react-hot-loader/babel",
    ["@babel/plugin-transform-runtime",
      {
        "regenerator": true
      }
    ]
  ]
}